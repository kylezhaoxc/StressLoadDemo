<Window x:Class="StressLoadDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StressLoadDemo"
        xmlns:view="clr-namespace:StressLoadDemo.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="Auto" Width="525"
        DataContext="{Binding Source={StaticResource Locator}, Path=Main}" SizeToContent="WidthAndHeight"
        >
    <Grid Height="Auto">
        <TabControl x:Name="TabControl" HorizontalAlignment="Left" Height="Auto" VerticalAlignment="Top" Width="517">
            <TabItem Header="TabItem">
                <view:TabRequirementView/>
            </TabItem>
            <TabItem Header="TabItem">
               <view:TabDashboardView/>
            </TabItem>
        </TabControl>


    </Grid>
</Window>
